{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/custom-tables",
  "version": "0.1.0",
  "title": "Custom Tables",
  "category": "design",
  "icon": "table-col-after",
  "description": "A customizable table block with color, size, and border options.",
  "keywords": [
    "table",
    "custom",
    "design"
  ],
  "example": {},
  "supports": {
    "html": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "textdomain": "custom-tables",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "rows": {
      "type": "array",
      "default": [
        [
          "Cell"
        ]
      ]
    },
    "textColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "borderColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "bgColor": {
      "type": "string",
      "default": "#12091a"
    },
    "fontSize": {
      "type": "number",
      "default": 16
    },
    "borderWidth": {
      "type": "number",
      "default": 1
    }
  }
}